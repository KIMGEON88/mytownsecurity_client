up($email: String!, $password: String!) {\n    signup(email: $email, password: $password)\n  }\n`;\n\nfunction SignUp({ isToken, setIsToken }) {\n  const history = useHistory();\n  const idInput = useInput('');\n  const passInput = useInput('');\n  const passConfirmInput = useInput('');\n  const [signUpMutation] = useMutation(SIGNUP, {\n    variables: {\n      email: idInput.value,\n      password: passInput.value,\n    },\n  });\n\n  const onSubmit = async (e) => {\n    e.preventDefault();\n    try {\n      if (\n        idInput.value !== '' &&\n        passInput.value !== '' &&\n        passConfirmInput.value !== ''\n      ) {\n        if (passInput.value !== passConfirmInput.value) {\n          alert('Please Check Password');\n        } else {\n          const { data: signup } = await signUpMutation();\n          if (signup) {\n            alert('Welocom to myTownSecurity');\n            history.push('/');\n          }\n        }\n      }\n    } catch (error) {\n      console.log(error);\n    }\n  };\n\n  return (\n    <>\n      {!isToken && (\n        <Background>\n          <Wrapper>\n            <a>회원가입</a>\n            <br />\n            <a>Email</a>\n            <form onSubmit={onSubmit}>\n              <SignupInput\n                type='email'\n                placeholder='이메일을 입력 해주세요.'\n                {...idInput}\n              />\n              <br />\n              <a>Password</a>\n              <br />\n              <SignupInput\n                type='password'\n                placeholder='비밀번호를 입력 해주세요.'\n                {...passInput}\n              />\n              <br />\n              <a>Confirm Password</a>\n              <br />\n              <SignupInput\n                type='password'\n                placeholder='비밀번호를 확인 해주세요.'\n                {...passConfirmInput}\n              />\n              <br />\n              <br />\n              <SignupButton>회원가입</SignupButton>\n            </form>\n          </Wrapper>\n        </Background>\n      )}\n      {isToken && null}\n    </>\n  );\n}\n\nexport default withRouter(SignUp);\n",[],"/Users/sohyeon/Desktop/Project/final project/mytownsecurity_client/src/components/modal/user/loginInput.js",[],"/Users/sohyeon/Desktop/Project/final project/mytownsecurity_client/src/components/myPage/MyPage.js",["185","186","187","188"],"/Users/sohyeon/Desktop/Project/final project/mytownsecurity_client/src/components/mainPage/Pagenation.js",["189","190"],"import React, { useState } from 'react';\n\nfunction UsePagination(data, itemsPerPage) {\n  const [currentPage, setCurrentPage] = useState(1);\n  const maxPage = Math.ceil(data.length / itemsPerPage);\n\n  function currentData() {\n    const begin = (currentPage - 1) * itemsPerPage;\n    const end = begin + itemsPerPage;\n    return data.slice(begin, end);\n  }\n\n  function next() {\n    setCurrentPage((currentPage) => Math.min(currentPage + 1, maxPage));\n  }\n\n  function prev() {\n    setCurrentPage((currentPage) => Math.max(currentPage - 1, 1));\n  }\n\n  function jump(page) {\n    const pageNumber = Math.max(1, page);\n    setCurrentPage((currentPage) => Math.min(pageNumber, maxPage));\n  }\n\n  return { next, prev, jump, currentData, currentPage, maxPage };\n}\n\nexport default UsePagination;\n","/Users/sohyeon/Desktop/Project/final project/mytownsecurity_client/src/components/servicePage/AddFavorite.js",[],"/Users/sohyeon/Desktop/Project/final project/mytownsecurity_client/src/components/myPage/MyFavoriteList.js",["191","192"],"/Users/sohyeon/Desktop/Project/final project/mytownsecurity_client/src/components/myPage/MyInfo.js",[],"/Users/sohyeon/Desktop/Project/final project/mytownsecurity_client/src/data/cctv.js",[],"/Users/sohyeon/Desktop/Project/final project/mytownsecurity_client/src/components/myPage/MyReviewList.js",["193","194"],"/Users/sohyeon/Desktop/Project/final project/mytownsecurity_client/src/styles/Modal.js",["195"],{"ruleId":"196","severity":1,"message":"197","line":2,"column":22,"nodeTyp